{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "globals": {
    "__LOGGER__": false,
    "SERVER_SIDE": false
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jasmine": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-uses-vars": 2,
    "camelcase": 0,
    "comma-dangle": [
      2,
      "always-multiline"
    ],
    "comma-spacing": 0,
    "consistent-return": 2,
    "constructor-super": 2,
    "curly": [
      2,
      "multi-line"
    ],
    "dot-notation": 2,
    "eol-last": 2,
    "eqeqeq": [
      2,
      "smart"
    ],
    "guard-for-in": 2,
    "handle-callback-err": 0,
    "indent": [
      2,
      "tab",
      { "SwitchCase": 1 }
    ],
    "key-spacing": 0,
    "keyword-spacing": 2,
    "linebreak-style": [
      2,
      "unix"
    ],
    "new-cap": 0,
    "new-parens": 0,
    "no-cond-assign": [
      2,
      "except-parens"
    ],
    "no-constant-condition": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-empty": 2,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extra-bind": 2,
    "no-extra-semi": 0,
    "no-loop-func": 2,
    "no-mixed-requires": 0,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-spaces": 0,
    "no-new-func": 2,
    "no-obj-calls": 2,
    "no-path-concat": 2,
    "no-process-env": 2,
    "no-process-exit": 2,
    "no-redeclare": 2,
    "no-return-assign": 0,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 0,
    "no-shadow": 0,
    "no-spaced-func": 0,
    "no-this-before-super": 2,
    "no-throw-literal": 2,
    "no-trailing-spaces": 2,
    "no-undef": 2,
    "no-underscore-dangle": 0,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unused-expressions": 0,
    "no-unused-vars": 2,
    "no-use-before-define": ["error", { "functions": false }],
    "no-wrap-func": 0,
    "quotes": [
      0,
      "double"
    ],
    "react/jsx-uses-react": 2,
    "semi": [
      0,
      "always"
    ],
    "semi-spacing": 2,
    "space-infix-ops": 0,
    "space-unary-ops": 2,
    "strict": [
      2,
      "never"
    ],
    "valid-typeof": 2,
    "yoda": 0
  }
}
