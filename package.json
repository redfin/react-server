{
  "private": true,
  "scripts": {
    "bootstrap": "npm i && npm run bootstrap-no-i",
    "//": "TODO: Drop `kill-singletons`... eventually",
    "bootstrap-no-i": "npm run kill-singletons && lerna bootstrap",
    "kill-singletons": "lerna exec -- rm -rf node_modules/react",
