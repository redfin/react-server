    "clean": "rimraf lerna-debug.log && lerna run clean",
    "nuke": "lerna clean && rm -r node_modules",
    "changelog": "lerna-changelog",
    "debug": "cat lerna-debug.log && for d in packages/*/npm-debug.log*; do echo $d; cat $d; done"
